<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    
    
    <span>
        Right Radius = <span id="i"><%= i %></span> mm
    </span><br/>
    <span>
        Left Radius = <span id="r"><%= r %></span> mm
    </span><br/>
    
    <span>
        Predicted Pressure Shaping = <span id="v">**</span> kg/cm2
    </span><br/>
    <span>
        Predicted Setting Radius = <span id="p">**</span> mm
    </span><br/>
    <span>
        Labels = <span id="l">**</span>
    </span>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    
    <script>	
	function classify(){
	  let i = $("#i").text();
	  let r = $("#r").text();
	  
          $.get(`/api/classify/${i}/${r}`).done(function(dt){
              
              $("#v").text(dt['jres'][0])
              $("#p").text(dt['jres'][1])
              $("#l").text(dt['jres_'])
              console.log(dt['jres'])
              console.log(dt['jres_'])
          });
	}
	
	classify();
	
        setInterval(function(){ 
        	classify();
        }, 5000);
    </script>
  

</body>
</html>
